{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET Core Launch (Blog.Web)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build Blog.Web",
        "program": "${workspaceFolder}/src/Blog.Web/bin/Debug/net8.0/Blog.Web.dll",
        "args": [],
        "cwd": "${workspaceFolder}/src/Blog.Web",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/src/Blog.Web/Views"
        },
        "justMyCode": true
      },
      {
        "name": ".NET Core Launch (Blog.Api)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build Blog.Api",
       "program": "${workspaceFolder}/src/Blog.Api/bin/Debug/net8.0/Blog.Api.dll",
        "args": [],
        "cwd": "${workspaceFolder}/src/Blog.Api",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/src/Blog.Api/Views"
        },
        "justMyCode": true
      }
    ]
  }